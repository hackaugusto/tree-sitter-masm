  u32checked_add
  u32checked_add.1
  push.TEST

  if.true end
  if.true assert end
  if.true assert else assert end

  while.true end
  while.true assert end

  repeat.5 end
  repeat.5 assert end

  call.test
  call.mem::memcopy

  exec.test
  exec.mem::memcopy

  syscall.test
  syscall.mem::memcopy
end

